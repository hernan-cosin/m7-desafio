  "license": "MIT",
  "source": "fe-src/index.html",
  "scripts": {
    "sync": "ts-node ./be-src/sync.ts",
    "dev": "yarn build:fe && yarn dev:be",
    "dev:fe": "parcel serve ./fe-src/index.html",
    "dev:be": "nodemon --watch ./be-src/index.ts --exec ts-node ./be-src/index.ts",
    "build:fe": "parcel build --dist-dir ./fe-dist",
    "build:be": "tsc ",
    "build": "npm run build:be && npm run build:fe",
    "start": "node ./be-dist"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "parcel": "^2.3.2",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "url": "^0.11.0"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^5.0.0",
    "@types/node": "^17.0.21",
    "@vaadin/router": "1.7.4",
    "algoliasearch": "^4.12.2",
    "cloudinary": "^1.28.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "dropzone": "^6.0.0-beta.2",
    "express": "4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mapbox": "^1.0.0-beta10",
    "mapbox-gl": "^2.7.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0"
  }
}
